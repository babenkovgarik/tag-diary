{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "jsx": "react-jsx",
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
        "*": ["types/*"]
      },
    },
    "exclude": ["node_modules", "dist"]
  }
  